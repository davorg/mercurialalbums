[% page_title = artist %]
<h1>[% artist %]</h1>
<ul class="list">
[% FOREACH a IN albums %]
  <li>
    [% IF a.is_winner %]<span class="badge">Winner</span>[% END %]
    <a href="[% a.url_path %]/">[% a.title %]</a> — <a href="[% a.year.url_path %]/">[% a.year.year %]</a>
  </li>
[% END %]
</ul>
<p><a href="/artist/">← All artists</a></p>
